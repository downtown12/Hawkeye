{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        },
        "plugins": [
            ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
            }]
        ]
      }
    ],
    "stage-2"
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "stage-2"
      ],
      "plugins": [
        "istanbul",
      ]
    }
  }
}
